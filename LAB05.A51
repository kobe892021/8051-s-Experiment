		ORG 0000H
START:	MOV A, #0xfC
		MOV SP, #32H
		MOV R4, #0
		JMP LOOP
		ORG 0030H
LOOP:	;MOV R1, #5D
		MOV P2, A
		RL A
		MOV P2, A
		CALL DELAY
		SETB P1.0
		SETB P1.1
		JNB P1.0, PUSH_FUN
		JNB P1.1, POP_FUN
		JMP LOOP	
PUSH_FUN:	CALL DELAY
			INC R4
			MOV R0, A
			PUSH 00H
			JMP LOOP
POP_FUN:	CALL DELAY
			POP 00H
			MOV A, R0
			MOV P2, A
			DJNZ R4, POP_FUN
			RET
DELAY:	MOV R5, #5
DEL1:	MOV R6, #200
DEL:	MOV R7, #230
		DJNZ R7, $
		DJNZ R6, DEL
		DJNZ R5, DEL1
		RET
		END